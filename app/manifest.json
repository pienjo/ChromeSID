{
  "manifest_version": 2,
  "name": "ChromeSID",
  "version": "1",
  "icons":{
    "128": "icon_128.png"
  },
  "permissions": [
    {
      "fileSystem" : [
        "retainEntries",
        "directory"
        ]
    },
    "storage"],
  "app": {
    "background": {
      "scripts": [ "background.js"]
    }
  },
  "offline_enabled": true,
  "file_handlers": {
    "sid": {
      "extensions": [
        "sid",
        "psid"
      ],
      "types": [
        "audio/x-sid",
        "audio/prs.sid"
      ]
    }
  },
  "minimum_chrome_version":"49"
}
